{
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "on",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "window.zoomLevel": 0,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.suggestSelection": "first",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"c:\\Users\\jianli\\.vscode\\extensions\\gabrielbb.vscode-lombok-1.0.1\\server\\lombok.jar\"",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.configuration.maven.userSettings": "D:\\apache-maven-3.5.0\\conf\\settings.xml",
    "maven.executable.path": "D:\\apache-maven-3.5.0\\bin\\mvn",
    "editor.fontSize": 16,
    "maven.excludedFolders": [
        "**/.*",
        "**/node_modules",
        "**/target",
        "**/bin"
    ],
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "java.errors.incompleteClasspath.severity": "ignore",
    "workbench.colorTheme": "Atom One Dark",
    "java.home": "D:\\java\\jdk",
    "maven.terminal.useJavaHome": true,
    "maven.terminal.customEnv": [
        {
            "environmentVariable": "JAVA_HOME",
            "value": "D:\\java\\jdk"
        }
    ],
    "java.semanticHighlighting.enabled": true,
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "files.associations": {
        "*.extension": "language",
        "/path to file/*.extension": "language"
    },
    //background扩展程序设置
    "background.enabled": true,
    "background.useDefault": false,
    "background.customImages": [
        // "file:///E:/image/阳光明媚的草地树鲜花.jpg"//图片路径
        // "file:///E:/image/阳光明媚的草地树鲜花.jpg"//图片路径
    ],
    "background.style": {
        "content": "''",
        "pointer-events": "none",
        "position": "absolute",
        "top": "0",
        "right": "0",
        "width": "100%",
        "height": "100%",
        "z-index": "99999",
        "background.repeat": "no-repeat",
        "background-size": "cover",
        "opacity": 0.15
    },
    //vscode底部状态栏设置
    "workbench.colorCustomizations": {
        "statusBar.background": "#3266c7",
        "statusBar.noFolderBackground": "#1aa157",
        "statusBar.debuggingBackground": "#c70d0d"
    },
    "workbench.statusBar.visible": true,
    //文件类型和编程语言
    "cSpell.enableFiletypes": [
        "bat",
        "clojure",
        "coffeescript",
        "concourse-pipeline-yaml",
        "concourse-task-yaml",
        "cshtml",
        "diff",
        "dockerfile",
        "fsharp",
        "git-rebase",
        "groovy",
        "hlsl",
        "ignore",
        "ini",
        "jinja",
        "jsx-tags",
        "jupyter",
        "log",
        "lua",
        "makefile",
        "manifest-yaml",
        "objective-c",
        "objective-cpp",
        "perl",
        "perl6",
        "pip-requirements",
        "powershell",
        "properties",
        "r",
        "razor",
        "ruby",
        "scminput",
        "search-result",
        "shaderlab",
        "shellscript",
        "spring-boot-properties",
        "spring-boot-properties-yaml",
        "sql",
        "swift",
        "toml",
        "vb",
        "vue",
        "vue-html",
        "vue-postcss",
        "xml",
        "xsl"
    ],
    
}